<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ask="http://schemas.android.com/apk/res-auto">

    <!-- Search keys -->
    <item ask:action_search="true" ask:key_type_feedback="true" android:drawable="@drawable/keyboard_search"/>
    <item ask:action_search="true" android:drawable="@drawable/keyboard_search"/>

    <!-- Done keys -->
    <item ask:action_done="true" ask:key_type_feedback="true" android:drawable="@drawable/keyboard_done"/>
    <item ask:action_done="true" android:drawable="@drawable/keyboard_done"/>

    <!-- ENTER keys -->
    <item ask:key_type_feedback="true" android:drawable="@drawable/enter"/>
    <item android:drawable="@drawable/enter"/>
</selector>